{

  "tags": {
    "allowUnknownTags": true
  },

  "opts": {
    "destination": "./docs",
    "template": "./node_modules/foodoc/template"
  },

  "sourceType": "module",

  "plugins": [
    "./node_modules/jsdoc-export-default-interop/dist/index.js",
    "plugins/markdown"
  ],

  "templates": {

    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },

    "dateFormat": "MMMM Do YYYY, H:mm:ss",

    "systemName": "DjipEvents",
    "systemSummary": "An event-handling library for the browser and Node.js",
    "copyright": "DjipEvents is released under the MIT license.",
    "collapseSymbols": true,

    "sort": "longname, linenum, version, since"

  }

}

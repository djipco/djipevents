{
  "name": "djipevents",
  "version": "0.9.9",
  "description": "A clean and opinionated event-handling library for the browser and Node.js.",
  "main": "dist/djipevents.cjs.min.js",
  "module": "dist/djipevents.esm.min.js",
  "author": "Jean-Philippe Côté <jp@djip.co>",
  "license": "MIT",
  "scripts": {
    "build-all": "npm run build-cjs && npm run build-esm && npm run build-iife",
    "build-cjs": "BABEL_ENV=cjs rollup --config",
    "build-esm": "BABEL_ENV=esm rollup --config",
    "build-iife": "BABEL_ENV=iife rollup --config",
    "test": "mocha --require @babel/polyfill ./test/index.js --exit",
    "test-coverage": "nyc --reporter=text npm run test"
  },
  "keywords": [
    "addEventListener",
    "addListener",
    "dispatch",
    "EventDispatcher",
    "EventEmitter",
    "EventTarget",
    "emit",
    "emitter",
    "event",
    "events",
    "listener",
    "observer",
    "observable",
    "publish",
    "subscribe",
    "trigger"
  ],
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "foodoc": "0.0.9",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^2.0.0",
    "jsdoc-export-default-interop": "^0.3.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-terser": "^5.0.0",
    "sinon": "^7.3.2"
  }
}
